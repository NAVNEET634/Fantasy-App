<%- include ('./adminheader.ejs') %>


<!--InsertDiv-->


  <div class="container-fluid mt--7">
      <div id="insertdiv">
    <div class="row">
      <div class="col-xl-12 order-xl-1">
        <div class="card bg-default shadow">
            <div class="card-header bg-default border-0">
                <div class="row align-items-center">
                  <div class="col-8">
                    <h3 class="mb-0" style="color:white">Add Matches</h3>
                  </div>
                
                </div>
              </div>
              
              <div class="card bg-default shadow">
            
           
          
          
                <div class="table-responsive">
                  <table class="table align-items-center table-dark table-flush">
                    <thead class="thead-dark">
                      <tr>
                        <th scope="col"> Match</th>
                      
                        <th scope="col"> League Name</th>
                      
                        <th scope="col">Time Left</th>

                        <th scope="col">User Joined</th>
                        
        
                        <th scope="col">LineUps Out</th>
                        <th scope="col"></th>
                      </tr>
                    </thead>
                    <tbody>
                          <tr>
                         <% for(i=0;i<result.length;i++){ %>
                            <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                  <span class="mb-0 text-sm"><%=result[i].team1name%> vs <%=result[i].team2name%></span>
                                </div>
                              </div>
                            </th>
                           
                           <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                  <span class="mb-0 text-sm"><%=result[i].leaguename%></span>
                                </div>
                              </div>
                            </th>
        
        
                   <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                  <span class="mb-0 text-sm"><%=result[i].timeleft%> minutes</span>
                                </div>
                              </div>
                            </th>
        
                            <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                  <span class="mb-0 text-sm"><%=result[i].lineup_status%></span>
                                </div>
                              </div>
                            </th>
        
                                  
                            <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                 <a href='/admin/user-joined?&matchid=<%=result[i].id%>'><button type="button" class="btn btn-dark">User Joined</button></a>
                                </div>
                              </div>
                            </th>
        
                            <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                 <a href='/admin/user-bet-joined?&matchid=<%=result[i].id%>'><button type="button" class="btn btn-dark">Bet User Joined</button></a>
                                </div>
                              </div>
                            </th>
                            
                            <th scope="row">
                                <div class="media align-items-center">
                                  <div class="media-body">
                                   <a href='/admin/lineups-out?team1id=<%=result[i].team1%>&team2id=<%=result[i].team2%>&matchid=<%=result[i].id%>'><button type="button" class="btn btn-danger">Lineup Out</button></a>
                                  </div>
                                </div>
                              </th>
         
                 
                              
                            <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                 <a href='/admin/fill-contests?team1id=<%=result[i].team1%>&team2id=<%=result[i].team2%>&matchid=<%=result[i].id%>'><button type="button" class="btn btn-dark">All Contests</button></a>
                                </div>
                              </div>
                            </th>
                            <th scope="row">
                              <div class="media align-items-center">
                                <div class="media-body">
                                 <a href='/admin/fill-contests?team1id=<%=result[i].team1%>&team2id=<%=result[i].team2%>&matchid=<%=result[i].id%>'><button type="button" class="btn btn-dark">Update Small Teams</button></a>
                                </div>
                              </div>
                            </th>
                          
                          </tr>
                         <%  } %>
                          
                    </tbody>
                      </table>
                    </div>
                  </div>
      </div>
    </div>
 </div>
</div>

<!--End InsertDiv-->



<!--UpdateImageDiv Start-->
<div id="updateimagediv">
    <div class="row">
      <div class="col-xl-12 order-xl-1">
        <div class="card bg-secondary shadow">
            <div class="card-header bg-white border-0">
                <div class="row align-items-center">
                  <div class="col-8">
                    <h3 class="mb-0">Update Team Logo</h3>
                  </div>
                  <div class="col-4 text-right">
                    <button type="button" id="back2" class="btn btn-sm btn-primary">Back</button>
                  </div>
                </div>
              </div>
              
          <div class="card-body">
            <form action="/teams/update_image" method="post" enctype="multipart/form-data">
              <div class="pl-lg-4">
                <div class="row">
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-username"> ID</label>
                      <input type="text" name="id" id="peid"  class="form-control form-control-alternative"
                        placeholder="ID" readonly>
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-email">Image</label>
                      <input type="file" name="logo"  class="form-control form-control-alternative">
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-12">
                    <div class="form-group">
                      <button type="submit" id="update" class="btn btn-info">Update Image</button>
                    </div>
                  </div>
                </div>
                </form>
          </div>
        </div>
      </div>
    </div>
  </div>
      </div>
  
  
      <!-- Footer -->
      <footer class="footer">
        <div class="row align-items-center justify-content-xl-between">
          <div class="col-xl-6">
            
          </div>
        
        </div>
      </footer>
    </div>
  </div>
  <!-- Argon Scripts -->
  <!-- Core -->
  <script src="/admin/assets/vendor/jquery/dist/jquery.min.js"></script>
  <script src="/admin/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <!-- Argon JS -->
  <script src="/admin/assets/js/argon.js?v=1.0.0"></script>
  
  <script type="text/javascript" src="/javascripts/matches.js"></script>



<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>
<script>
$('.uploadImage').submit(function(e){
    e.preventDefault();
     $(this).ajaxSubmit({
      contentType: 'application/json',
      success: function(response){
     alert("Successfully")
    
      
      }
  });
    return false;
});
</script>




</body>

</html>